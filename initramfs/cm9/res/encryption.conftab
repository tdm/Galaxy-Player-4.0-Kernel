# mount point   device name   device mapper path   device mapper name   file sysystem   protect type
/dbdata /dev/block/mtdblock5 /dev/mapper/dbdata dbdata yaffs2 NOT_PROTECTED 
/data /dev/block/mmcblk0p2 /dev/mapper/data data ext4 NOT_PROTECTED 
/sdcard /dev/block/mmcblk0p1 /dev/mapper/sdcard sdcard vfat NOT_PROTECTED 
/cache /dev/block/mtdblock6 /dev/mapper/cache cache yaffs2 NOT_PROTECTED 
#/data/data /dev/block/mmcblk0p3 /dev/mapper/dataprivate dataprivate yaffs2 PROTECTED_WITHOUT_PASSWORD 
/ext_sdcard /dev/block/mmcblk1 /dev/mapper/extsdcard sdcard1 vfat PROTECTED_WITH_PASSWORD 
/efs /dev/block/mtdblock0 /dev/mapper/efs efs yaffs2 NOT_PROTECTED
